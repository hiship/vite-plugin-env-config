{
  "name": "vite-env-config",
  "version": "0.0.6",
  "description": "A Vite plugin to generate config.js from .env file",
  "type": "module",
  "main": "./vite/index.js",
  "types": "./vite/index.d.ts",
  "exports": {
    ".": {
      "import": "./vite/index.js",
      "default": "./vite/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "tsc-alias": "^1.8.8",
    "typescript": "^4.9.5",
    "vite": "^6.2.4"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "env",
    "config"
  ],
  "author": "hiship",
  "license": "MIT"
}
